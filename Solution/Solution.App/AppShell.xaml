<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Solution.App.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:Solution.App.Views"
    Title="Solution.App">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Add Page"
        ContentTemplate="{DataTemplate views:AddTodoPage}"
        Route="AddTodoPage" />
</Shell>
